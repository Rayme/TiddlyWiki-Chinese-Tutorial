{
  "description": "太微中文教程",
  "plugins": [
    "tiddlywiki/filesystem",
    "tiddlywiki/tiddlyweb",
    "tiddlywiki/highlight",
    "tiddlywiki/pluginlibrary",
    "tiddlywiki/codemirror",
    "tiddlywiki/codemirror-autocomplete",
    "tiddlywiki/codemirror-closebrackets",
    "tiddlywiki/codemirror-closetag",
    "tiddlywiki/codemirror-fullscreen-editing",
    "tiddlywiki/codemirror-keymap-sublime-text",
    "tiddlywiki/codemirror-mode-css",
    "tiddlywiki/codemirror-mode-htmlmixed",
    "tiddlywiki/codemirror-mode-javascript",
    "tiddlywiki/codemirror-mode-markdown",
    "tiddlywiki/codemirror-mode-xml",
    "tiddlywiki/codemirror-search-replace",
    "tiddlywiki/dynannotate",
    "tiddlywiki/dynaview",
    "tiddlywiki/highlight",
    "tiddlywiki/katex",
    "tiddlywiki/markdown",
    "tiddlywiki/sax",
    "tiddlywiki/text-slicer",
    "tiddlywiki/qrcode",
    "Gk0Wk/TW5-CodeMirror-Enhanced",
    "Gk0Wk/sidebar-resizer",
    "Gk0Wk/notionpage-covericon",
    "Gk0Wk/notionpage-backlink",
    "oflg/fishing",
    "linonetwo/commandpalette",
    "linonetwo/copy-on-select",
    "linonetwo/github-external-image",
    "linonetwo/google-calendar-import",
    "linonetwo/inverse-link-and-folder",
    "linonetwo/itonnote",
    "linonetwo/opened-tiddlers-bar",
    "linonetwo/pinyin-fuzzy-search",
    "linonetwo/prevent-edit",
    "linonetwo/preview-glass",
    "linonetwo/service-worker",
    "linonetwo/source-control-management",
    "linonetwo/sub-wiki",
    "linonetwo/zx-script",

    "tobibeer/appear",
    "bimlas/locator",
    "bimlas/kin-filter",
    "dullroar/sitemap",
    "felixhayashi/tiddlymap",
    "felixhayashi/vis",
    "felixhayashi/hotzone",
    "felixhayashi/topstoryview",
    "mat/field-value-selector",
    "sobjornstad/TiddlyRemember",
    "telmiger/EditorCounter",
    "telmiger/HarveyBalls",
    "telmiger/PluginSize"
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "linonetwo/itonnote"
  ],
  "languages": [
    "zh-Hans"
  ],
  "build": {
    "index": [
      "--rendertiddler",
      "$:/plugins/tiddlywiki/tiddlyweb/save/offline",
      "index.html",
      "text/plain"
    ],
    "externalimages": [
      "--setfield",
      "[is[binary]] [type[application/msword]]",
      "_canonical_uri",
      "$:/core/templates/canonical-uri-external-image",
      "text/plain",
      "--setfield",
      "[is[binary]] [type[application/msword]]",
      "text",
      "",
      "text/plain",
      "--rendertiddler",
      "$:/core/save/all-external-js",
      "index.html",
      "text/plain"
    ],
    "externaljs": [
      "--rendertiddler",
      "$:/core/templates/tiddlywiki5.js",
      "tiddlywiki5.js",
      "text/plain"
    ]
  }
}
